   <extension
         point="org.eclipse.ui.commands">
      <command
            description="Validate Interfaces between selected components"
            id="org.polarsys.capella.core.data.cs.validation2.validateInterfaces"
            name="Validate Component Interfaces">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="org.polarsys.capella.core.data.cs.validation2.ValidateInterfacesHandler"
            commandId="org.polarsys.capella.core.data.cs.validation2.validateInterfaces">
         <activeWhen>
            <and>
               <count
                     value="(1-2)">
               </count>
               <iterate>
                  <adapt
                        type="org.eclipse.emf.ecore.EObject">
                     <instanceof
                           value="org.polarsys.capella.core.data.cs.Component">
                     </instanceof>
                  </adapt>
               </iterate>
            </and>
         </activeWhen>
      </handler>
